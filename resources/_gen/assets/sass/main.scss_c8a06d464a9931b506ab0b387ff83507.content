  font-family: "Inter", sans-serif;
  color: var(--neutral-700);
  box-shadow: 0px 6px 5px rgba(0, 0, 0, 0.012), 0px 22px 18px rgba(0, 0, 0, 0.018), 0px 100px 80px rgba(0, 0, 0, 0.03);
  border-radius: 1rem;
  block-size: 100%;
  line-height: 1.25;
}
.card > :last-child {
  margin-block-start: auto;
}
.card__heading {
  font-size: var(--fs-600);
  color: var(--neutral-700);
  font-family: "Montserrat", sans-serif;
  text-transform: none;
  letter-spacing: 0;
  font-weight: 700;
  line-height: 1.1;
}

.heading-1 {
  font-size: var(--fs-900);
  color: var(--brand-primary-700);
  font-family: "Montserrat", sans-serif;
  text-transform: none;
  letter-spacing: 0;
  font-weight: 700;
  line-height: 1.1;
}

.heading-2 {
  font-size: var(--fs-800);
  color: var(--brand-primary-700);
  font-family: "Montserrat", sans-serif;
  text-transform: none;
  letter-spacing: 0;
  font-weight: 700;
  line-height: 1.1;
}

.heading-3 {
  font-size: var(--fs-700);
  color: var(--neutral-100);
  font-family: "Montserrat", sans-serif;
  text-transform: none;
  letter-spacing: 0;
  font-weight: 700;
  line-height: 1.1;
}

.header {
  background-color: var(--brand-primary-100);
  color: var(--neutral-700);
  padding-block: 1.5rem;
  padding-inline: 0;
  block-size: 112px;
}
.header .logo {
  inline-size: 60px;
  block-size: 60px;
}
.header__inner {
  --cluster-space: 2rem;
  --cluster-justification: space-between;
  background-color: var(--brand-primary-100);
}
@media screen and (min-width: 45em) {
  .header__inner {
    justify-content: space-between;
  }
}

.nav {
  position: fixed;
  inset-inline: 0;
  z-index: 1;
  inset-block-start: 112px;
  transition: all 0.5s ease-in;
  background-color: var(--brand-primary-100);
  display: grid;
  grid-template-rows: 0fr;
  opacity: 0;
}
.nav > * {
  overflow: hidden;
}
@media screen and (min-width: 45em) {
  .nav {
    position: static;
    display: block;
    opacity: 1;
  }
}
.nav ul {
  --flex-column-gap: 2rem;
  font-family: "Inter", sans-serif;
  font-weight: 400;
  list-style: none;
  text-align: center;
  display: flex;
  flex-flow: column nowrap;
  gap: var(--flex-column-gap);
}
@media screen and (min-width: 45em) {
  .nav ul {
    flex-flow: row wrap;
    --flex-column-gap: 2.5rem;
  }
}
.hamburger-menu:checked ~ .nav {
  grid-template-rows: 1fr;
  opacity: 1;
  padding-block-end: 2rem;
}
.nav a {
  color: var(--neutral-700);
  text-decoration: none;
}
.nav a:is(:hover, :focus) {
  color: var(--neutral-700);
}

.footer {
  background-color: var(--neutral-100);
  color: var(--neutral-700);
  font-size: var(--fs-500);
}
.footer__inner > * {
  list-style: none;
}
.footer__inner > * {
  padding-block: 2rem;
  border-block-end: 2px solid var(--neutral-200);
}
.footer__inner > :first-child {
  padding-block: 0 2rem;
}
.footer__inner > :last-child {
  padding-block: 2rem 0;
  border: none;
}
.footer .links {
  --cluster-gap: 1.5rem;
  --cluster-justification: center;
}
.footer .copyright {
  --cluster-gap: 1.5rem;
  --cluster-justification: center;
}
.footer a {
  color: var(--neutral-700);
  text-decoration: none;
  font-family: "Inter", sans-serif;
  letter-spacing: 0;
}
.footer a:is(:hover, :focus) {
  color: var(--neutral-700);
}

.hamburger-menu-icon {
  stroke: var(--neutral-700);
}
.hamburger-menu-icon:hover {
  cursor: pointer;
}
@media screen and (min-width: 45em) {
  .hamburger-menu-icon {
    display: none;
  }
}

