@font-face {
  font-family: "Montserrat";
  font-weight: 700;
  font-display: swap;
  src: url("/fonts/montserrat-v29-latin-700.woff2") format("woff2");
}
@font-face {
  font-family: "Inter";
  font-weight: 400;
  font-display: swap;
  src: url("/fonts/inter-v18-latin-regular.woff2") format("woff2");
}
@font-face {
  font-family: "Inter";
  font-weight: 700;
  font-display: swap;
  src: url("/fonts/inter-v18-latin-700.woff2") format("woff2");
}
:root {
  --neutral-100: hsl(0, 0%, 100%);
  --neutral-200: hsl(0, 0%, 91%);
  --neutral-400: hsl(0, 0%, 42%);
  --neutral-700: hsl(0, 0%, 25%);
  --brand-primary-100: hsl(213, 47%, 96%);
  --brand-primary-200: hsl(218, 39%, 86%);
  --brand-primary-400: hsl(218, 48%, 39%);
  --brand-primary-700: hsl(218, 36%, 23%);
  --brand-accent-100: hsl(168, 98%, 81%);
  --brand-accent-200: hsl(168, 96%, 51%);
  --brand-accent-400: hsl(169, 100%, 41%);
  --brand-accent-700: hsl(168, 80%, 19%);
  --yellow-300: hsl(36, 100%, 86%);
  --yellow-400: hsl(36, 80%, 65%);
  --yellow-500: hsl(36, 77%, 30%);
  --pink-300: hsl(340, 81%, 84%);
  --pink-400: hsl(340, 68%, 63%);
  --pink-500: hsl(340, 76%, 27%);
  --cyan-300: hsl(194, 98%, 81%);
  --cyan-400: hsl(197, 64%, 53%);
  --cyan-500: hsl(195, 69%, 27%);
  --fs-900: 2.625rem;
  --fs-800: 2rem;
  --fs-700: 1.5rem;
  --fs-600: 1.375rem;
  --fs-500: 1.125rem;
  --fs-400: 1rem;
  --fs-300: 0.925rem;
  --fs-200: 0.875rem;
}
@media screen and (min-width: 65em) {
  :root {
    --fs-900: 3.5rem;
    --fs-800: 2.625rem;
    --fs-700: 1.5rem;
    --fs-600: 1.375rem;
    --fs-500: 1.125rem;
    --fs-400: 1rem;
    --fs-300: 0.925rem;
    --fs-200: 0.875rem;
  }
}

*,
*::before,
*::after {
  box-sizing: border-box;
}

* {
  margin: 0;
  padding: 0;
  font: inherit;
}

img,
picture,
svg {
  display: block;
  max-width: 100%;
}

html:focus-within {
  scroll-behavior: smooth;
}

input,
button,
textarea,
select {
  font: inherit;
}

:is(h1, h2, h3, h4, h5, h6) {
  line-height: 1.1;
}

.sr-only:not(:focus):not(:active) {
  clip: rect(0 0 0 0);
  clip-path: inset(50%);
  height: 1px;
  overflow: hidden;
  position: absolute;
  white-space: nowrap;
  width: 1px;
}

@media (prefers-reduced-motion: reduce) {
  html:focus-within {
    scroll-behavior: auto;
  }
  *,
  *::before,
  *::after {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
    scroll-behavior: auto !important;
  }
}
body {
  font-family: "Inter", sans-serif;
  font-weight: 400;
  font-size: var(--fs-500);
  color: var(--neutral-400);
  background-color: var(--brand-primary-100);
  line-height: 1.5;
}
body:has(#hamburger-menu:checked) {
  overflow: hidden;
}

a {
  color: var(--brand-primary-400);
}

a:is(:hover, :focus) {
  color: var(--brand-accent-700);
}

svg {
  inline-size: 4rem;
  block-size: 4rem;
}

strong {
  font-weight: 700;
}

main {
  isolation: isolate;
}

/*! purgecss start ignore */
.button {
  display: inline-block;
  align-content: center;
  cursor: pointer;
  text-decoration: none;
  line-height: 1;
  font-family: "Inter", sans-serif;
  font-size: var(--fs-500);
  text-transform: none;
  font-weight: 400;
  padding-inline: 2em;
  padding-block: 0.75em;
  border: 0;
  border-radius: 0.5rem;
  transition: 275ms ease;
}
.button[data-type=primary] {
  background-color: var(--brand-primary-400);
  color: var(--neutral-100);
}
.button[data-type=primary]:is(:hover, :focus) {
  background-color: var(--brand-primary-200);
  color: var(--brand-primary-700);
}
.button[data-type=accent] {
  background-color: var(--brand-accent-400);
  color: var(--brand-primary-700);
}
.button[data-type=accent]:is(:hover, :focus) {
  background-color: var(--brand-accent-200);
  color: var(--brand-primary-700);
}
.button[data-type=link] {
  background-color: var(--neutral-100);
  color: var(--brand-primary-400);
}
.button[data-type=link]:is(:hover, :focus) {
  background-color: var(--neutral-200);
  color: var(--brand-primary-400);
}

/*! purgecss end ignore */
.features {
  --flow-space: 2rem;
}
.features p {
  max-inline-size: 35ch;
}
.features .wrapper {
  display: grid;
  gap: 1rem;
  grid-auto-rows: 280px;
}
@media screen and (min-width: 45em) {
  .features .wrapper {
    grid-template-columns: repeat(3, 1fr);
  }
}
.features .creation {
  background-color: var(--brand-accent-100);
  position: relative;
  z-index: 1;
}
.features .creation::after {
  content: "";
  position: absolute;
  inset: 0;
  background: var(--brand-accent-400);
  -webkit-mask-image: url("/images/book.svg");
  mask-image: url("/images/book.svg");
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  -webkit-mask-position: right -25px bottom -90px;
  mask-position: right -25px bottom -90px;
  -webkit-mask-size: 200px;
  mask-size: 200px;
  z-index: 0;
  pointer-events: none;
}
.features .creation a {
  color: var(--brand-accent-700);
}
.features .creation a:is(:hover, :active) {
  color: var(--brand-accent-700);
}
.features .target {
  background-color: var(--yellow-300);
  position: relative;
  z-index: 1;
}
.features .target::after {
  content: "";
  position: absolute;
  inset: 0;
  background: var(--yellow-400);
  -webkit-mask-image: url("/images/advertising-icon.svg");
  mask-image: url("/images/advertising-icon.svg");
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  -webkit-mask-position: right -25px bottom -90px;
  mask-position: right -25px bottom -90px;
  -webkit-mask-size: 200px;
  mask-size: 200px;
  z-index: 0;
  pointer-events: none;
}
.features .target a {
  color: var(--yellow-500);
}
.features .target a:is(:hover, :active) {
  color: var(--yellow-500);
}
@media screen and (min-width: 45em) {
  .features .target {
    grid-column: 2/-1;
  }
}
.features .social {
  background-color: var(--pink-300);
  position: relative;
  z-index: 1;
}
.features .social::after {
  content: "";
  position: absolute;
  inset: 0;
  background: var(--pink-400);
  -webkit-mask-image: url("/images/social-media-icon.svg");
  mask-image: url("/images/social-media-icon.svg");
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  -webkit-mask-position: right -25px bottom -90px;
  mask-position: right -25px bottom -90px;
  -webkit-mask-size: 200px;
  mask-size: 200px;
  z-index: 0;
  pointer-events: none;
}
.features .social a {
  color: var(--pink-500);
}
.features .social a:is(:hover, :active) {
  color: var(--pink-500);
}
@media screen and (min-width: 45em) {
  .features .social {
    grid-column: 1/3;
  }
}
.features .analytics {
  background-color: var(--cyan-300);
  position: relative;
  z-index: 1;
}
.features .analytics::after {
  content: "";
  position: absolute;
  inset: 0;
  background: var(--cyan-400);
  -webkit-mask-image: url("/images/analytics-icon.svg");
  mask-image: url("/images/analytics-icon.svg");
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  -webkit-mask-position: right -25px bottom -90px;
  mask-position: right -25px bottom -90px;
  -webkit-mask-size: 200px;
  mask-size: 200px;
  z-index: 0;
  pointer-events: none;
}
.features .analytics a {
  color: var(--cyan-500);
}
.features .analytics a:is(:hover, :active) {
  color: var(--cyan-500);
}

.card {
